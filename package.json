{
  "name": "backk-frontend-utils",
  "version": "1.0.60",
  "description": "Web frontend utils for Backk microservices",
  "author": {
    "name": "Petri Silen",
    "email": "petri.silen@silensoft.com"
  },
  "engines": {
    "node": ">= 12.19"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/backk-node/backk-frontend-utils.git"
  },
  "bugs": {
    "url": "https://github.com/backk-node/backk-frontend-utils/issues"
  },
  "keywords": [
    "backk",
    "microservice",
    "frontend",
    "web",
    "utils"
  ],
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "tsc -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint ./src --ext .ts --fix",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build",
    "prepare": "husky install"
  },
  "dependencies": {
    "class-transformer": "0.4.0",
    "crypto-js": "4.1.1",
    "cv-pksilen": "0.13.5",
    "dayjs": "1.10.7",
    "js-base64": "3.7.2",
    "lodash": "4.17.21",
    "reflect-metadata": "0.1.13"
  },
  "devDependencies": {
    "@types/crypto-js": "4.0.2",
    "@types/lodash": "4.14.176",
    "@types/react": "17.0.34",
    "@typescript-eslint/eslint-plugin": "5.2.0",
    "@typescript-eslint/parser": "5.2.0",
    "eslint": "8.1.0",
    "eslint-config-prettier": "8.3.0",
    "husky": "^7.0.4",
    "jest": "27.3.1",
    "lint-staged": "11.2.6",
    "prettier": "2.4.1",
    "react": "17.0.2",
    "typescript": "4.4.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "node",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.ts": [
      "npm run format",
      "npm run lint"
    ]
  }
}
